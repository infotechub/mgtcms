@{
    ViewBag.Title = "Register";
}

@{
    Layout = null;
}

<!DOCTYPE HTML>
<html>
<head>
    <title>Novo Clinic Management System</title>
    <link href="~/Content/css/style.css" rel="stylesheet" type="text/css" media="all" />
