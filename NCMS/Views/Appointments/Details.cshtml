                                        <div class="table-responsive">
                                            <table class="table">
                                                <tr>
                                                    <font size="5">
                                                        VITAL DETAILS
                                                    </font>
                                                    <hr />

                                                    <td>
                                                        <b> <font size="3">     @Html.DisplayNameFor(model => model.BloodPresure) </font></b>
                                                    </td>
                                                    <td>
                                                        @{
                                                            Html.DisplayFor(model => model.BloodPresure);
                                                            var BP = Model.BloodPresure;
                                                        }
                                                        @if (BP < 130.80 && BP > 90.60)
                                                        {
                                                            <p style='font-size: 12pt; font-family: arial; font-weight: bold; background-color:green; color:white'>
                                                                @BP.ToString("000.00")


                                                            </p>


                                                        }
                                                        else if (BP > 139.80 && BP == 150.90)
                                                        {
                                                            <p style='font-size: 12pt; font-family: arial; font-weight: bold; background-color:yellow; color:white'>
                                                                @BP.ToString("000.00")


                                                            </p>

                                                        }
                                                        else if (BP > 150.90)
                                                        {
                                                            <p style='font-size: 12pt; font-family: arial; font-weight: bold; background-color:red; color:white'>
                                                                @BP.ToString("000.00")


                                                            </p>

                                                        }
                                                        else if (BP < 90.60)
                                                        {
                                                            <p style='font-size: 12pt; font-family: arial; font-weight: bold; background-color:black; color:white'>
                                                                @BP.ToString("000.00")


                                                            </p>

                                                        }

                                                        else
                                                        {
                                                            <p style='font-size: 12pt; font-family: arial; font-weight: bold;'>
                                                                Not Available
                                                            </p>

                                                        }



                                                    </td>
                                                </tr>
